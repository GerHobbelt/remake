 DIST_COMMON = $(make_TEXINFOS) $(srcdir)/Makefile.in Makefile.am \
 	make.1.in stamp-vti version-mdb.texi
 all: all-am
@@ -437,7 +436,7 @@
 	pdf-am ps ps-am uninstall uninstall-am uninstall-info-am
 
 
-all: mdb.info make.info
+all: mdb.info
 
 $(srcdir)/version-mdb.texi: $(srcdir)/stamp-vti
 
