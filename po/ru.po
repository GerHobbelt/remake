msgid "  -h, --help                   Print this message and exit.\n"
msgstr "  -h, --help                   Показать эту справку и выйти.\n"

#: src/main.c:377
msgid "  -i, --ignore-errors          Ignore errors from commands.\n"
msgstr "  -i, --ignore-errors          Игнорировать ошибки команд.\n"

#: src/main.c:379
msgid ""
"  -I DIRECTORY, --include-dir=DIRECTORY\n"
"                               Search DIRECTORY for included makefiles.\n"
msgstr ""
"  -I КАТАЛОГ, --include-dir=КАТАЛОГ\n"
"                               КАТАЛОГ поиска включаемых makefile-ов.\n"

#: src/main.c:382
msgid ""
"  -j [N], --jobs[=N]           Allow N jobs at once; infinite jobs with no "
"arg.\n"
msgstr ""
"  -j [N], --jobs[=N]           Запускать одновременно до N заданий; если\n"
"                               аргумент не задан, число заданий не "
"ограничено.\n"

#: src/main.c:384
msgid ""
"  -k, --keep-going             Keep going when some targets can't be made.\n"
msgstr ""
"  -k, --keep-going             Продолжать работу, даже если некоторые цели\n"
"                               не могут быть достигнуты.\n"

#: src/main.c:386
msgid ""
"  -l [N], --load-average[=N], --max-load[=N]\n"
"                               Don't start multiple jobs unless load is "
"below N.\n"
msgstr ""
"  -l [N], --load-average[=N], --max-load[=N]\n"
"                               Не запускать сразу несколько задач,\n"
"                               если загрузка не ниже N.\n"

#: src/main.c:389
msgid ""
"  -L, --check-symlink-times    Use the latest mtime between symlinks and "
"target.\n"
msgstr ""
"  -L, --check-symlink-times    Использовать последнее mtime при выборе "
"между\n"
"                               символическими ссылками и целью.\n"

#: src/main.c:391
msgid ""
"  -n, --just-print, --dry-run, --recon\n"
"                               Don't actually run any commands; just print "
"them.\n"
msgstr ""
"  -n, --just-print, --dry-run, --recon\n"
"                               Не выполнять команды на самом деле; просто\n"
"                               напечатать их.\n"

#: src/main.c:394
msgid ""
"  --no-extended-errors         Do not give additional error reporting.\n"
msgstr ""
"  --no-extended-errors         Не показывать развёрнутые сообщения об "
"ошибках.\n"

#: src/main.c:396
msgid ""
"  -o FILE, --old-file=FILE, --assume-old=FILE\n"
"                               Consider FILE to be very old and don't remake "
"it.\n"
msgstr ""
"  -o ФАЙЛ, --old-file=ФАЙЛ, --assume-old=ФАЙЛ\n"
"                               Считать ФАЙЛ очень старым и не переделывать "
"его.\n"

#: src/main.c:399
msgid "  -p, --print-data-base        Print make's internal database.\n"
msgstr ""
"  -p, --print-data-base        Напечатать внутреннюю базу данных make.\n"

#: src/main.c:401
msgid ""
"  -q, --question               Run no commands; exit status says if up to "
"date.\n"
msgstr ""
"  -q, --question               Не выполнять команды;\n"
"                               код завершения покажет, всё ли уже сделано.\n"

#: src/main.c:403
msgid "  -r, --no-builtin-rules       Disable the built-in implicit rules.\n"
msgstr ""
"  -r, --no-builtin-rules       Не использовать встроенные неявные правила.\n"

