msgstr "  -v, --version                Показать информацию о версии и выйти.\n"

#: src/main.c:415
msgid "  -V, --show-variables         Show variable expansions.\n"
msgstr "  -V, --show-variables         Показать значения переменных.\n"

#: src/main.c:417
msgid "  -w, --print-directory        Print the current directory.\n"
msgstr "  -w, --print-directory        Напечатать текущий каталог.\n"

#: src/main.c:419
msgid ""
